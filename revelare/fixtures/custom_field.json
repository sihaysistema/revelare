  "insert_after": "waze_map",
  "label": "waze_map_iframe",
  "length": 0,
  "mandatory_depends_on": null,
  "modified": "2016-11-21 11:53:03.517253",
  "name": "Address-waze_map_iframe",
  "no_copy": 0,
  "non_negative": 0,
  "options": "<div>\n<iframe id=\"wazeframe1\" src=\"https://embed.waze.com/es/iframe?zoom=12&lat=14.53926&lon=-90.73005&width=290&height=440&pin=1\" width=\"300\" height=\"450\" frameborder=\"0\">\n</iframe>\n<br>\n<button onclick=\"wazeManipulate()\">Centrar Mapa en Direcci\u00f3n</button>\n<br>\n<button onclick=\"setLatLonData()\">Pegar datos a campos de latitud y longitud</button>\n<p name=\"wazenavlink\">waze://?z=12?ll=14.53926,-90.73005&navigate=yes</p>\n<script>\nvar latitude_validate = cur_frm.doc.waze_lat;\nvar longitude_validate = cur_frm.doc.waze_lon;\nvar default_lat = 14.53857; //default location latitude\nvar default_lon = -90.73153; // default location longitude\nvar lat = default_lat;\nvar lon = default_lon;\nvar pure_lat = default_lat;\nvar pure_lon = default_lon;\nvar zoom_default = 12;\nvar zoom_coords = 17;\nvar zoom_assigned = 12;\nvar width_default = \"290\";\nvar width_assigned = \"290\";\nvar max_widthERP = 725;\nvar window_pad_px = 50;\nvar mobile_pad_px = 10;\nvar height_default = \"450\";\nvar height_assigned = \"450\";\nvar pin_yesno = 1;\nvar pin_assigned = 1;\nvar alert_no_coords = \"Por favor seleccione una ubicacion en el mapa, y copie el permalink en el campo de \\\"Permalink\\\", o ingr\u00e9se coordenadas manualmente. Luego reintente.\";\nvar navigate_with_waze;\n//Waze LINKS  for instant navigation and other cool stuff.\n// Search for and address: waze://?q=address_search_term;\nvar address_search_term;\n// Center the map to latitude/longitude: waze://?ll=lat,lon\n// Navigate to a specific point (lat/lon): waze://?ll=lat,lon&navigate=yes\nvar waze_navigate = \"yes\";  //or no\n// Set the zoom when loading: waze://?z=zoom_assigned\n//A full URL with navigation and zoom setup: waze://?z=zoom_assigned?ll=lat,lon&navigate=yes\n\n\n\nvar window_width_px = window.innerWidth\n|| document.documentElement.clientWidth\n|| document.body.clientWidth;\n\nfunction wazeInitialMap() {\n  /* get the latitude and longitude from the fields in ERPNext\n  */\n  var latitude_validate = cur_frm.doc.waze_lat;\n  var longitude_validate = cur_frm.doc.waze_lon;\n  \n  if (latitude_validate === undefined || longitude_validate === undefined) {\n    var lat = default_lat;\n    var lon = default_lon;\n    zoom_assigned = zoom_default;   \n  } else {\n    var lat = cur_frm.doc.waze_lat; // get the value from waze_lat field\n    var lon = cur_frm.doc.waze_lon; // get the value from waze_lon field\n    zoom_assigned = zoom_coords;\n  };\n  // In case the window size was adjusted, this refreshes the variable\n  window_width_px = window.innerWidth\n  || document.documentElement.clientWidth\n  || document.body.clientWidth;\n  // this if, ensures that the waze section is properly displayed\n  if (window_width_px > max_widthERP) {\n    width_assigned = max_widthERP - window_pad_px;\n  } else {\n    width_assigned = window_width_px - mobile_pad_px;\n  };\n  \n  var srcpart0 = \"https://embed.waze.com/es/iframe?zoom=\";\n  var srcpart1 = \"&lat=\";\n  var srcpart2 = \"&lon=\";\n  var srcpart3 = \"&width=\";\n  var srcpart4 = \"&height=\";\n  var srcpart5 = \"&pin=\";\n  var embed_url = srcpart0 + zoom_assigned + srcpart1 + lat + srcpart2 + lon + srcpart3 + width_assigned + srcpart4 + height_assigned + srcpart5 + pin_assigned;\n  document.getElementById(\"wazeframe1\").src = embed_url;\n  document.getElementById(\"wazeframe1\").width = width_assigned;\n  var contents1 = document.getElementById(\"wazeframe1\").src;\n  \n  //Navigate with Waze link  FIXME\n  var wazeurlpart0 = \"waze://?z=\";\n  var wazeurlpart1 = \"?ll=\";\n  var wazeurlpart2 = \",\";\n  var wazeurlpart3 = \"&navigate=\";\n  var embed_waze_url = wazeurlpart0 + zoom_assigned + wazeurlpart1 + lat + wazeurlpart2 + lon + wazurlpart3 + waze_navigate;\n  //document.getElementById(\"wazenavlink\").href = embed_waze_url;\n  document.getElementById(\"wazenavlink\").innerHTML = embed_waze_url;\n};\n\nfunction wazeManipulate() {\n  /* get the latitude and longitude from the fields in ERPNext,  FIXME  (Add a validator, in case Lat and Lon are BEYOND a certain value, return Fogliasana coordinates)\n  */\n  var latitude_validate = cur_frm.doc.waze_lat;\n  var longitude_validate = cur_frm.doc.waze_lon;\n  if (latitude_validate === undefined || longitude_validate === undefined) {\n    var lat = pure_lat; //usually set to default, or with new values from getData()\n    var lon = pure_lon;\n    alert(alert_no_coords);\n    zoom_assigned = zoom_default;\n  } else {\n    var lat = cur_frm.doc.waze_lat; // get the value from waze_lat field\n    var lon = cur_frm.doc.waze_lon; // get the value from waze_lon field\n    zoom_assigned = zoom_coords;\n  };\n  // In case the window size was adjusted, this refreshes the variable\n  window_width_px = window.innerWidth\n  || document.documentElement.clientWidth\n  || document.body.clientWidth;\n  if (window_width_px > max_widthERP) {\n    width_assigned = max_widthERP - window_pad_px;\n  } else {\n    width_assigned = window_width_px - mobile_pad_px;\n  };\n  // Now redo the embed link\n  var srcpart0 = \"https://embed.waze.com/es/iframe?zoom=\";\n  var srcpart1 = \"&lat=\";\n  var srcpart2 = \"&lon=\";\n  var srcpart3 = \"&width=\";\n  var srcpart4 = \"&height=\";\n  var srcpart5 = \"&pin=\";\n  var embed_url = srcpart0 + zoom_assigned + srcpart1 + lat + srcpart2 + lon + srcpart3 + width_assigned + srcpart4 + height_assigned + srcpart5 + pin_assigned;\n  document.getElementById(\"wazeframe1\").src = embed_url;\n  document.getElementById(\"wazeframe1\").width = width_assigned;\n  var contents1 = document.getElementById(\"wazeframe1\").src;\n  \n  // Change the permalink.\n  var linkpart0 = \"https://www.waze.com/es/livemap?zoom=\";\n  var linkpart1 = \"&lat=\";\n  var linkpart2 = \"&lon=\";\n  var linkpart5 = \"&pin=\";\n  var live_waze_url = linkpart0 + zoom_assigned + linkpart1 + lat + linkpart2 + lon + linkpart5 + pin_assigned;\n  cur_frm.set_value(\"waze_permalink\", live_waze_url);\n    \n  //Navigate with Waze link\n  var wazeurlpart0 = \"waze://?z=\";\n  var wazeurlpart1 = \"?ll=\";\n  var wazeurlpart2 = \",\";\n  var wazeurlpart3 = \"&navigate=\";\n  var embed_waze_url = wazeurlpart0 + zoom_assigned + wazeurlpart1 + lat + wazeurlpart2 + lon + wazurlpart3 + waze_navigate;\n  //document.getElementById(\"wazenavlink\").href = embed_waze_url;\n  document.getElementById(\"wazenavlink\").innerHTML = embed_waze_url;\n};\n\nfunction setLatLonData() {\n  // FIXME  IF YOU PRESS THIS TWICE, IT CAUSES TROUBLE\n  var permalink_validate = cur_frm.doc.waze_permalink;\n  if (permalink_validate === undefined) {\n    alert(alert_no_coords);\n  } else {\n      var iframe_src_string = cur_frm.doc.waze_permalink;\n      var pos_lat = iframe_src_string.search(\"&lat=\");\n      var pos_lon = iframe_src_string.search(\"&lon=\");\n      // the length of the string\n      var iframe_src_string_length = iframe_src_string.length;\n      // extract a substring that only includes from lat, not the ampersand before it, but all the way to the end of the string.\n      var iframe_substring = iframe_src_string.substring(pos_lat+1, iframe_src_string_length);\n      // extract a subarray that only includes lat and lon parameters, but dirty. They begin with lat=  or lon=\n      var iframe_subarray = iframe_substring.split(\"&\",2);\n      // Now, we assign each array to a variable, to remove the first four characters\n      var lat_from_array = iframe_subarray[0];\n      var lon_from_array = iframe_subarray[1];\n      // remove the first four characters, to finally get only latitude and longitude.\n      pure_lat = lat_from_array.substring(4, lat_from_array.length);\n      pure_lon = lon_from_array.substring(4, lon_from_array.length);\n      // Assign these values to the input forms that will house the lat and lon to be modified\n      cur_frm.set_value(\"waze_lat\", pure_lat);\n      cur_frm.set_value(\"waze_lon\", pure_lon);      \n      wazeManipulate(); // Since coordinates are now set, recenter the map\n  };\n};\n</script>\n<style onload=\"wazeInitialMap()\"></style>\n</div>",
